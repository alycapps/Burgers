<h1>Burgers to be Eaten</h1>
<ul>
    {{#each burger}}
        {{#unless devoured}}
              {{this.burger_name}}
        {{/unless}}
    {{/each}}
</ul>

<h1>Burgers that have been Eaten</h1>
<ul>
    {{#each burger}}
        {{#if devoured}}
              {{this.burger_name}}
        {{/if}}
    {{/each}}
</ul>