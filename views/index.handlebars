<h1>Burgers to be Eaten</h1>
<ul>
    {{#each burgers}}
        {{#unless devoured}}
            <li>
                {{this.burger_name}}.{{this.burger}}
                <button data-id="{{this.id}}" class="devourbttn">Devour</button>
            </li>
        {{/unless}}
    {{/each}}
</ul>

<h1>Burgers that have been Eaten</h1>
<ul>
    {{#each burgers}}
        {{#if devoured}}
              {{this.burger_name}}.{{this.burger}}
        {{/if}}
    {{/each}}
</ul>